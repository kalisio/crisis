  "scripts": {
    "version": "cordova-set-version src-cordova/config.xml.tpl",
    "release:patch": "cd api && npm run release:patch && cd .. && npm version --no-git-tag-version --force patch",
    "release:minor": "cd api && npm run release:minor && cd .. && npm version --no-git-tag-version --force minor",
    "release:major": "cd api && npm run release:major && cd .. && npm version --no-git-tag-version --force major",
    "changelog": "github_changelog_generator -u kalisio -p aktnmap && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "clean": "quasar clean",
    "provision": "npm run icons:cp && npm run logos:cp && npm run tours:cp",
    "dev": "npm run provision && cross-env NODE_ENV=development quasar dev",
    "prod": "cd api && npm run prod",
    "build": "npm run provision && quasar build",
    "cordova:clean": "cd src-cordova && cordova clean && cd ..",
    "cordova:config": "envsub -s dollar-both src-cordova/config.xml.tpl src-cordova/config.xml",
    "cordova:config:android": "npm run cordova:config && cd src-cordova && cordova platform add android && cd ..",
    "cordova:config:ios": "npm run cordova:config && cd src-cordova && cordova platform add ios && cd ..",
    "cordova:dev:android": "npm run cordova:config:android && npm run provision && quasar dev -m android",
    "cordova:dev:ios": "npm run cordova:config:ios && npm run provision && quasar dev -m ios",
    "cordova:build:android": "npm run cordova:config:android && npm run provision && quasar build -m cordova -T android",
    "cordova:build:ios": "npm run cordova:config:ios && npm run provision && quasar build -m cordova -T ios -- --device",
    "analyze:stats": "webpack --config build/webpack.prod.conf.js --profile --json > build-stats.json",
    "analyze:treemap": "webpack-bundle-analyzer build-stats.json dist",
    "analyze": "npm run analyze:stats && npm run analyze:treemap",
    "lint": "standard src/**/*.js src/**/*.vue test/**/*.js tutorials/**/*.js --fix",
    "benchmark": "node ./benchmark",
    "icons:cp": "shx cp -R node_modules/@kalisio/kdk/extras/icons/* public/icons/kdk",
    "logos:cp": "shx cp -R node_modules/@kalisio/kdk/extras/logos/* public/",
    "tours:cp": "shx cp -R node_modules/@kalisio/kdk/extras/tours/* src/tours",
    "mocha": "mocha test/**/*.test.js --timeout 30000 --require esm",
    "coverage": "c8 npm run mocha",
    "test:server": "cd api && npm run test",
    "test:client": "cd test && npm run coverage",
    "clean-databases": "node ./tools/clean-databases",
    "clean-sns": "node ./tools/clean-sns"
  },
  "standard": {
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint",
      "allowImportExportEverywhere": true,
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "plugins": [
      "vue"
    ],
    "env": [
      "mocha"
    ],
    "ignore": [
      "public"
    ],
    "globals": [
      "DEV",
      "PROD",
      "__THEME",
      "test",
      "fixture",
      "fetch",
      "cordova",
      "location",
      "FileReader",
      "LocalFileSystem",
      "mime",
      "atob",
      "URL",
      "Blob",
      "Event"
    ]
  },
