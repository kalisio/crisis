{
  "name": "aktnmap",
  "description": "Kalisio Akt'n'Map Application",
  "version": "0.8.0",
  "homepage": "https://github.com/kalisio/aktnmap",
  "keywords": [
    "kalisio"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kalisio/aktnmap.git"
  },
  "author": {
    "name": "Kalisio contributors",
    "url": "https://github.com/kalisio"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/kalisio/aktnmap/issues"
  },
  "scripts": {
    "version": "cordova-set-version cordova/config.xml.tpl",
    "release:patch": "cd api && npm run release:patch && cd .. && npm version --no-git-tag-version --force patch",
    "release:minor": "cd api && npm run release:minor && cd .. && npm version --no-git-tag-version --force minor",
    "release:major": "cd api && npm run release:major && cd .. && npm version --no-git-tag-version --force major",
    "changelog": "github_changelog_generator -u kalisio -p aktnmap && git add CHANGELOG.md && git commit -am \"Updating changelog\" && git push origin",
    "clean": "quasar clean",
    "dev": "cross-env NODE_ENV=development quasar dev",
    "prod": "cd api && npm run prod",
    "build": "quasar build",
    "cordova:clean": "cd src-cordova && cordova clean && cd ..",
    "cordova:dev:android": "envsub -s dollar-both src-cordova/config.xml.tpl src-cordova/config.xml && quasar dev -m android",
    "cordova:dev:ios": "envsub -s dollar-both src-cordova/config.xml.tpl src-cordova/config.xml && quasar dev -m ios",
    "cordova:build:android": "envsub -s dollar-both src-cordova/config.xml.tpl src-cordova/config.xml && quasar build -m cordova -T android",
    "cordova:build:ios": "envsub -s dollar-both src-cordova/config.xml.tpl src-cordova/config.xml && quasar build -m cordova -T ios -- --device",
    "analyze:stats": "webpack --config build/webpack.prod.conf.js --profile --json > build-stats.json",
    "analyze:treemap": "webpack-bundle-analyzer build-stats.json dist",
    "analyze": "npm run analyze:stats && npm run analyze:treemap",
    "lint": "standard src/**/*.js src/**/*.vue test/**/*.js tutorials/**/*.js --fix",
    "benchmark": "node ./benchmark",
    "cafe": "testcafe firefox test/*.test.js -S -s screenshots",
    "rec": "node ./tutorials/record.js",
    "test:server": "cd api && npm run test",
    "test:client": "cross-env NODE_ENV=production testcafe chrome:headless test/*.test.js --app \"npm run prod\" --app-init-delay 10000 -S -s screenshots",
    "test": "npm-run-all --parallel test:**",
    "clean-databases": "node ./tools/clean-databases",
    "clean-sns": "node ./tools/clean-sns"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "testcafe",
      "html"
    ],
    "sourceType": "module",
    "globals": [
      "DEV",
      "PROD",
      "__THEME",
      "cordova",
      "test",
      "fixture"
    ]
  },
  "dependencies": {
    "@feathersjs/client": "^3.5.4",
    "@panter/vue-i18next": "^0.9.1",
    "@quasar/extras": "^1.2.0",
    "Leaflet.Geodesic": "https://github.com/henrythasler/Leaflet.Geodesic.git",
    "ajv": "^5.2.2",
    "ajv-i18n": "2",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.25.0",
    "casl": "^1.0.2",
    "chart.js": "^2.8.0",
    "chartjs-plugin-annotation": "^0.5.7",
    "chroma-js": "^2.0.2",
    "cross-env": "^5.2.0",
    "fastclick": "^1.0.6",
    "feathers-authentication-management": "^1.0.0",
    "feathers-client": "^2.4.0",
    "feathers-reactive": "^0.8.1",
    "formatcoords": "^1.1.3",
    "georaster": "^0.4.2",
    "i18next": "^10.5.0",
    "jquery": "^3.2.1",
    "js-yaml": "^3.13.1",
    "leaflet": "^1.2.0",
    "leaflet-draw": "^1.0.4",
    "leaflet-fa-markers": "^0.0.5",
    "leaflet-filelayer": "^1.0.0",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-pixi-overlay": "^1.5.1",
    "leaflet-realtime": "https://github.com/kalisio/leaflet-realtime",
    "leaflet-timedimension": "^1.1.0",
    "leaflet-velocity": "^1.2.2",
    "leaflet.locatecontrol": "^0.67.0",
    "leaflet.markercluster": "^1.0.5",
    "leaflet.vectorgrid": "^1.3.0",
    "lodash": "^4.17.15",
    "loglevel": "^1.6.0",
    "mathjs": "^5.7.0",
    "mime-types-browser": "^0.0.3",
    "moment": "^2.24.0",
    "papaparse": "^4.6.1",
    "password-generator": "^2.2.0",
    "pixi.js": "^4.8.5",
    "quasar": "^1.0.5",
    "showdown": "^1.8.6",
    "sift": "^6.0.0",
    "socket.io-client": "^2.0.1",
    "togeojson": "^0.16.0",
    "vue": "^2.6.10",
    "vue-stripe-elements-plus": "^0.2.8",
    "vue2-dropzone": "^3.6.0",
    "weacast-core": "^1.0.0",
    "weacast-leaflet": "^1.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "clui": "^0.3.6",
    "colors": "^1.1.2",
    "commander": "^2.15.0",
    "config": "^1.26.1",
    "connect-history-api-fallback": "^1.1.0",
    "cordova": "^8.0.0",
    "cordova-set-version": "^6.0.3",
    "envsub": "^3.0.9",
    "es6-promise": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "glob": "^7.1.2",
    "mongodb": "^3.1.4",
    "node-fetch": "^2.2.0",
    "npm-run-all": "^4.1.2",
    "opn": "^5.0.0",
    "shelljs": "^0.8.1",
    "sns-mobile": "https://github.com/kalisio/sns-mobile#protocol",
    "standard": "^14.0.0",
    "testcafe": "^1.0.1",
    "testcafe-vue-selectors": "^3.0.0",
    "winston": "^3.2.1",
    "worker-farm": "^1.6.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
